        Else
            Text1(Label0.Caption).Text = Text1(Label0.Caption).Text & Command0(Index).Caption
        End If
    Else
        If Command0(Index).Caption = "a > A" Then
            For iCount = 23 To 53
                Command0(iCount).Caption = UCase$(Command0(iCount).Caption)
            Next iCount
            Command0(54).Caption = ";"
            Command0(55).Caption = ":"
            Command0(56).Caption = "_"
            Command0(Index).Caption = "A > a"
        Else
            For iCount = 23 To 53
                Command0(iCount).Caption = LCase$(Command0(iCount).Caption)
            Next iCount
            Command0(54).Caption = ","
            Command0(55).Caption = "."
            Command0(56).Caption = "-"
            Command0(Index).Caption = "a > A"
        End If
    End If
    
    If Label0.Caption = 4 Then
        Combo2.SetFocus
    Else
        Text1(Label0.Caption).SetFocus
    End If
    
