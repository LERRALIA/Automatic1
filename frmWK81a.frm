Private Sub Command1_Click(Index As Integer)
    On Error GoTo LOKAL_ERROR
    
    Dim iRet As Integer
    Dim sNR As String
    
    Select Case Index
        Case Is = 0     'Speichern
            iRet = fnPruefeEingabeWK81a()
            Select Case iRet
                Case Is = 0     'alles okay
                    SchreibeDatenWK81a
                    fuellecombo
                Case Is = 1     'keine Bezeichnung
                    MsgBox "Bitte den Text angeben!", vbInformation, "Winkiss Hinweis:"
                    Text1(0).SetFocus
                Case Is = 2     'keine Dauer
                    MsgBox "Bitte die Behandlungsdauer angeben!", vbInformation, "Winkiss Hinweis:"
                    Text1(1).SetFocus
                Case Is = 3     'Dauer zu groß
                    MsgBox "Der eingegebene Wert ist zu groß!", vbInformation, "Winkiss Hinweis:"
                    Text1(1).SetFocus
                Case Is = 4     'keine Nummer
                    MsgBox "Bitte geben Sie eine Nummer an!", vbInformation, "Winkiss Hinweis:"
                    Text1(2).SetFocus
                Case Is = 5    'schon vorhanden, Bezeichnung
                    MsgBox "Diese Bezeichnung ist leider schon vergeben!", vbInformation, "Winkiss Hinweis:"
                    Text1(0).SetFocus
            End Select
        Case Is = 1     'Auswählen
            If MSHFLEX1.Row < 1 Then
                Screen.MousePointer = 0
                MsgBox "Bitte einen Satz in der Tabelle markieren!", vbInformation, "Winkiss Hinweis:"
                Exit Sub
            End If
            sNR = Trim(MSHFLEX1.TextMatrix(MSHFLEX1.Row, SpaltennummerNR))
            
            HoleDatenWK81a sNR
            
        Case Is = 2     'Löschen
            If MSHFLEX1.Row < 1 Then
                Screen.MousePointer = 0
                MsgBox "Bitte einen Satz in der Tabelle markieren!", vbInformation, "Winkiss Hinweis:"
                Exit Sub
            End If
            sNR = Trim(MSHFLEX1.TextMatrix(MSHFLEX1.Row, SpaltennummerNR))
            LoescheDatenWK81a sNR
        Case Is = 8     'drucken
        
            drucke_Behandlungstexte
            
        Case Is = 5     'Feld leeren
            If Label0.Caption = 4 Then
                Combo2.Text = ""
                Combo2.SetFocus
            Else
        
                Text1(Label0.Caption).Text = ""
                Text1(Label0.Caption).SetFocus
            End If
        Case Is = 6     'Beenden
            Unload frmWK81a
        Case Is = 7
            gsZSpalte = "nr"
            gstab = "TERM_STD"
            frmWKL36.Show 1
            'fertig
    End Select
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Command1_Click"
    Fehler.gsFehlertext = "Im Programmteil Vorgaben Behandlungen ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
