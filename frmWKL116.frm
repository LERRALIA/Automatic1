Private Sub Command1_Click(Index As Integer)
On Error GoTo LOKAL_ERROR
    
    Select Case Index
        Case 0 'Zahlung vornehmen
            Zahlung_elPAY txtBetrag.Text
        Case 1 'Storno vornehmen
            Storno_elPAY txtTANR.Text, txtBetrag.Text
        Case 2
            If Lebst_Du_noch_ELPAY() Then
                MsgBox "Ja"
            Else
                MsgBox "Nein"
            End If
        
        Case 3
            Unload frmWKL116
        Case 4
            speicher_ELPAY
            lese_ELPAY_opt
        Case 5 'Diagnose
            Diagnose_elPAY
        Case 6 'Kassenschnitt
            Kassenschnitt_elPAY
    End Select
    
    Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
