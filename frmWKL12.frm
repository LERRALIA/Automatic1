    sSQL = "Insert into AGNPRINT select AGN  "
    sSQL = sSQL & ", AGTEXT "
    sSQL = sSQL & ", INVAB  from AGNDBF "
    gdBase.Execute sSQL, dbFailOnError
    
    
    If Check1.Value = vbChecked Then
        Set rsrs = gdBase.OpenRecordset("AGNPRINT")
        If Not rsrs.EOF Then
            rsrs.MoveFirst
            Do While Not rsrs.EOF
    
                If Not IsNull(rsrs!AGN) Then
                    rsrs.Edit
                    rsrs!Artikelanz = ermAnzArtProAGN(rsrs!AGN)
                    rsrs!Bestandsum = ermsumbestProAGN(rsrs!AGN)
                    rsrs.Update
