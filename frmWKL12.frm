            rsrs.MoveNext
            Loop
    
        End If
        rsrs.Close: Set rsrs = Nothing
    End If
    
    loeschNEW "agntemp", gdBase
    sSQL = "select * into agntemp from  AGNPRINT"
    gdBase.Execute sSQL, dbFailOnError
    
    loeschNEW "AGNPRINT", gdBase
    CreateTableT2 "AGNPRINT", gdBase
    sSQL = "Insert into AGNPRINT select * from AGNtemp "
    
    If Option1(0).Value Then
         sSQL = sSQL & " order by  AGN "
    ElseIf Option1(1).Value Then
         sSQL = sSQL & " order by AGTEXT "
    ElseIf Option1(2).Value Then
        sSQL = sSQL & " order by Artikelanz desc "
    ElseIf Option1(3).Value Then
        sSQL = sSQL & " order by Bestandsum desc "
    End If
    gdBase.Execute sSQL, dbFailOnError
    
    If gbSQLSERVER Then
    
        Hintergrundtabelle_kopieren "AGNPRINT"
        dbPrintOpen
    End If
    
    reportbildschirm "dWKL12a", "aWKL12a"
    
