Private Function SchreibeDatenWKL12() As Boolean
    On Error GoTo LOKAL_ERROR
    
    Dim cSQL    As String
    Dim rsrs    As Recordset
    Dim rs1     As Recordset
    Dim cFeld   As String
    Dim dFeld   As Double
    Dim cAGTE   As String
    Dim iRet    As Integer
    
    SchreibeDatenWKL12 = False
    
    cFeld = Trim$(Text1(0).Text)
    cAGTE = Trim(UCase(Text1(1).Text))
    
'    If CLng(cFeld) > 999 And CLng(cFeld) < 2000 Then
'        anzeige "rot", "Artikelgruppen im Bereich 1000 - 2000 können nicht bearbeitet werden.", lblAnzeige
'        Exit Function
'    End If
    
    cSQL = "Select * from AGNDBF where AGN = " & cFeld & " "
    Set rsrs = gdBase.OpenRecordset(cSQL)
    
    If Not rsrs.EOF Then
        rsrs.Edit
    Else
        rsrs.AddNew
        cSQL = "Select agtext from AGNDBF where Ucase(AGtext) = '" & cAGTE & "' "
        Set rs1 = gdBase.OpenRecordset(cSQL)
        If Not rs1.EOF Then
            iRet = MsgBox("Diese Artikelgruppenbezeichnung ist schon vergeben." & vbCrLf & "Wirklich speichern?", vbQuestion + vbYesNo, "Winkiss Frage:")
            If iRet = vbNo Then
                Text1(1).SetFocus
                rs1.Close: Set rs1 = Nothing
                Exit Function
            End If
        End If
        rs1.Close: Set rs1 = Nothing
    End If
    
    cFeld = Text1(0).Text
    rsrs!AGN = cFeld
    rsrs!AGTEXT = cAGTE
    
    dFeld = CDbl(Text1(2).Text)
    
    If Val(dFeld) > 100 Then
        anzeigeNew "rot", "Der Abschlag liegt über 100 Prozent.", lblAnzeige
        Text1(1).SetFocus
        Exit Function
    End If
    
    rsrs!INVAB = dFeld
    rsrs!LASTDATE = DateValue(Now)
    rsrs!LASTTIME = TimeValue(Now)
    
    rsrs.Update
    rsrs.Close: Set rsrs = Nothing
