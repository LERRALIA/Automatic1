Private Sub FuelleListbox2WKL12(corder As String)
    On Error GoTo LOKAL_ERROR
    
    Dim cSQL        As String
    Dim rsrs        As Recordset
    Dim dWert       As Double
    Dim cFeld       As String
    Dim cLBSatz     As String
    Dim lAnz        As Long
    
    List2.Clear
    
    cSQL = "Select * from AGNDBF  " & corder
    Set rsrs = gdBase.OpenRecordset(cSQL)
    
    If Not rsrs.EOF Then
        rsrs.MoveLast
        lAnz = rsrs.RecordCount
        rsrs.MoveFirst
        Do While Not rsrs.EOF
            If Not IsNull(rsrs!AGN) Then
                dWert = rsrs!AGN
            Else
                dWert = 0
            End If
            cFeld = Format$(dWert, "#####0")
            cLBSatz = Space(6 - Len(cFeld)) & cFeld & " "
            
