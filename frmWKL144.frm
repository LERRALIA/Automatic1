Private Sub insertSpezpreis(lartnr As Long, bytePreistyp As Byte, siPreiswert As Single)
    On Error GoTo LOKAL_ERROR
    
    Dim sSQL As String

    sSQL = "Delete from Preise where ARTNR = " & lartnr
    sSQL = sSQL & " and Preistyp = " & bytePreistyp
    gdBase.Execute sSQL, dbFailOnError
    
    If siPreiswert > 0 Then
    
        sSQL = "Insert into Preise (Artnr,Preistyp,Preiswert,AENDER) values  "
        sSQL = sSQL & " ( " & lartnr
        sSQL = sSQL & " , " & bytePreistyp
        sSQL = sSQL & " , '" & siPreiswert & "'"
        sSQL = sSQL & " , '" & DateValue(Now) & "'"
        sSQL = sSQL & " )"
        gdBase.Execute sSQL, dbFailOnError
        
    End If
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "insertSpezpreis"
    Fehler.gsFehlertext = "Im Programmteil spezieller Kassenpreis ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub

Private Function LeseDatum(lartnr As Long, bytePreistyp As Byte) As Long
    On Error GoTo LOKAL_ERROR
    
    Dim sSQL As String
    Dim rsrs As Recordset
    
    LeseDatum = 0

    sSQL = "Select * from Preise where ARTNR = " & lartnr
    sSQL = sSQL & " and Preistyp = " & bytePreistyp
    Set rsrs = gdBase.OpenRecordset(sSQL)
    If Not rsrs.EOF Then
        If Not IsNull(rsrs!AENDER) Then
            LeseDatum = rsrs!AENDER
        End If
    End If
    rsrs.Close: Set rsrs = Nothing
    
Exit Function
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "LeseDatum"
    Fehler.gsFehlertext = "Im Programmteil spezieller Kassenpreis ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Function
Private Sub Form_Load()
    On Error GoTo LOKAL_ERROR
    
    Dim lDatum As Long
    Dim siPreis As Single
    
    Modul6.Skalieren Me, True, True: Modul6.Schrift Me: Modul6.Log Me
    Modul6.alternativFarbform Me, Nothing
    
    If Not NewTableSuchenDBKombi("PREISE", gdBase) Then
        CreateTableT2 "PREISE", gdBase
        CheckIndex "PREISE", "ARTNR", "", gdBase
    End If
    
    If gsARTNR = "" Then
        Exit Sub
    End If
    
    Label1(1).Caption = gsARTNR
    lDatum = LeseDatum(CLng(gsARTNR), 0)
    If lDatum > 0 Then
        Label1(3).Visible = True
        Label1(2).Caption = Format(lDatum, "DD.MM.YY")
    Else
        Label1(3).Visible = False
        Label1(2).Caption = ""
    End If
    
    siPreis = LeseSpezpreis(CLng(gsARTNR), 0)
    If siPreis > 0 Then
        Text1.Text = Format(siPreis, "######0.00")
    End If
    
Exit Sub
