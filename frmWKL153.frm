Private Sub Command5_Click(Index As Integer)
On Error GoTo LOKAL_ERROR

    Dim sPfad As String

    Select Case Index
        Case 0
            Unload frmWKL153
        Case 1
            'Futura
            frmWKL152.Show 1
        Case 2
            'EWWS Teil 2
            frmWKL155.Show 1
        Case 3
            'SQL-DABA-Import
            frmWKL197.Show 1

        Case 6
            'EWWS
            frmWKL151.Show 1
            
        Case 4
            'dewas-Import
            frmWKL199.Show 1
        Case 5
            'Quisy-Import
            frmWKL200.Show 1
        Case 7
            'Schapfl-Import
            frmWKL202.Show 1
    End Select

err:
Exit Sub
