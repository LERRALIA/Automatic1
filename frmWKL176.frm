Private Sub Command1_Click(Index As Integer)
    On Error GoTo LOKAL_ERROR
    
    Select Case Index
        Case 0 To 9
            Text1.Text = Text1.Text & Command1(Index).Caption
            Text1.SetFocus
        Case 10
            Text1.Text = ""
            Text1.SetFocus
        Case 11
            If IsNumeric(Command1(Index).Caption) Then
                Text1.Text = Command1(Index).Caption
            Else
                MsgBox "Geben Sie bitte über die Schaltfläche 'E' die Voreinstellungen für die Belegung der Schaltflächen an!", vbOKOnly + vbInformation, "Winkiss Hinweis:"
            End If
            Text1.SetFocus
        Case 12
            If IsNumeric(Command1(Index).Caption) Then
                Text1.Text = Command1(Index).Caption
            Else
                MsgBox "Geben Sie bitte über die Schaltfläche 'E' die Voreinstellungen für die Belegung der Schaltflächen an!", vbOKOnly + vbInformation, "Winkiss Hinweis:"
            End If
            Text1.SetFocus
        Case 13
            
            Text1.Text = "99999"
            
            SpeicherPLZ
            Unload frmWKL176
            
    End Select

    
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Command1_Click"
    Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
Private Sub Command2_Click(Index As Integer)
On Error GoTo LOKAL_ERROR

    Screen.MousePointer = 11
    
    Select Case Index
        Case 0      'Speichern
        
            If Len(Text1.Text) <> 5 Then
                MsgBox "Bitte geben Sie eine 5stellige Postleitzahl ein!"
            Else
                SpeicherPLZ
                Unload frmWKL176
            End If
        Case 1 'Einstellung
            Command1(11).Caption = InputBox("Hier die Voreinstellung für die 1. PLZ angeben.", "Winkiss Frage:", "Voreinstellung vornehmen")
            Command1(12).Caption = InputBox("Hier die Voreinstellung für die 2. PLZ angeben.", "Winkiss Frage:", "Voreinstellung vornehmen")
            If IsNumeric(Command1(11).Caption) Then
                speicherVorEinPlz1
            Else
                Command1(11).Caption = "PLZ 1"
            
            End If
            
            If IsNumeric(Command1(12).Caption) Then
                speicherVorEinPlz2
            Else
                Command1(12).Caption = "PLZ 2"
            
            End If
        Case 2      'abbrechen
            Unload frmWKL176
            
    End Select
    Screen.MousePointer = 0
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Command2_Click"
    Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
