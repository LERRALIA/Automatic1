Private Sub ZeigeZugehörigeRechte(byteRecht As Byte, cbedname)
    On Error GoTo LOKAL_ERROR

    Dim cSQL        As String
    Dim rsrs        As Recordset
    Dim cFeld       As String
    
    List4.Clear
    List4.AddItem "Programmteil"
    List3.Clear
    
    loeschNEW "BEDZU1", gdBase
    cSQL = "Select * into BEDZU1 from BEDZUGRI where ZUGRIFF <= " & byteRecht
    cSQL = cSQL & " order by ZUGRIFFNR "
    gdBase.Execute cSQL, dbFailOnError
    
    cSQL = "Select * from BEDZU1 "
    cSQL = cSQL & " order by ZUGRIFFNR "
    Set rsrs = gdBase.OpenRecordset(cSQL)
    If Not rsrs.EOF Then
        rsrs.MoveFirst
        Do While Not rsrs.EOF
            If Not IsNull(rsrs!PROTEIL) Then
                cFeld = rsrs!PROTEIL
            End If
            List3.AddItem cFeld
            rsrs.MoveNext
        Loop
    End If
    rsrs.Close: Set rsrs = Nothing
    Label1(5).Caption = cbedname & " darf " & List3.ListCount & " Programmteile nutzen."
    Label1(5).Refresh
    
    Frame30.Visible = True
    
Exit Sub
