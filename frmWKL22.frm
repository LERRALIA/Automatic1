    Fehlermeldung1
End Sub
Private Function ErmittleNeueBednummer() As String
    On Error GoTo LOKAL_ERROR
    
    Dim cSQL As String
    Dim rsrs As Recordset
    Dim lMaxNr As Long
    Dim i       As Long
    Dim lFilVon As Long
    Dim lFilbis As Long
    Dim sFilvon As String
    Dim sFilbis As String
    Dim rsrs1 As Recordset
    Dim iRet As Integer
    Dim ctmp As String
    
    ErmittleNeueBednummer = ""

    lFilVon = 1
    lFilbis = 999
    For i = lFilVon To lFilbis
hier:
        cSQL = "Select BEDNU from BEDNAME where BEDNU = " & i
        Set rsrs = gdBase.OpenRecordset(cSQL)

        If rsrs.EOF Then 'ist recset am ende = keinen gefunden
            cSQL = "Select max(adate)as maxi from Kassjour where bediener = " & i
            Set rsrs1 = gdBase.OpenRecordset(cSQL)
            
            If Not rsrs1.EOF Then
            
                rsrs1.MoveFirst
                If Not IsNull(rsrs1!maxi) Then
                    ctmp = "Als nächstfreie Bedienernummer wurde die " & i & " ermittelt." & vbCrLf
                    ctmp = ctmp & "Mit dieser Bedienernummer wurde am " & rsrs1!maxi & " das letzte Mal ein Verkauf getätigt." & vbCrLf
                     ctmp = ctmp & "Möchten Sie die vorgeschlagene Bedienernummer trotzdem vergeben."
                    iRet = MsgBox(ctmp, vbInformation + vbYesNo, "Winkiss Hinweis:")
                    If iRet = vbYes Then
                        Exit For
                    Else
                        If i < lFilbis Then
                            i = i + 1
                            GoTo hier
                        Else
                            ErmittleNeueBednummer = ""
                        End If
                        
                    End If
                Else
                    Exit For
                End If
            Else 'auf jedenFall frei und ohne Verkauf
                Exit For
            End If
            rsrs1.Close: Set rsrs1 = Nothing
        End If
        rsrs.Close: Set rsrs = Nothing
    Next i

    ErmittleNeueBednummer = Format$(i, "##0")

    Exit Function
