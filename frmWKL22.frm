Private Sub Command4_Click(Index As Integer)
    On Error GoTo LOKAL_ERROR
    
    Dim iZielIndex As Integer
    Dim lcount As Long
    
    iZielIndex = Label3(2).Caption
        
    Select Case Index
        Case Is = 0     'CLEAR
            Text1(iZielIndex).Text = ""
            Text1(iZielIndex).SetFocus
        Case Is = 1     'BACKSPACE
            If Len(Text1(iZielIndex).Text) > 0 Then
                Text1(iZielIndex).Text = Left(Text1(iZielIndex).Text, Len(Text1(iZielIndex).Text) - 1)
            End If
            Text1(iZielIndex).SetFocus
        Case Is = 2     'BEFORE
            If iZielIndex > 0 Then
                Text1(iZielIndex).BackColor = vbWhite
                iZielIndex = iZielIndex - 1
                Label3(2).Caption = Trim$(Str$(iZielIndex))
                Text1(iZielIndex).BackColor = glSelBack1
            End If
            Text1(iZielIndex).SetFocus
        Case Is = 3     'NEXT
            If iZielIndex < 5 Then
                Text1(iZielIndex).BackColor = vbWhite
                iZielIndex = iZielIndex + 1
                Label3(2).Caption = Trim$(Str$(iZielIndex))
                Text1(iZielIndex).BackColor = glSelBack1
            End If
            Text1(iZielIndex).SetFocus
        Case Is = 4     'switch to lower case / UPPER CASE
            SwitchUpperLowerCaseWKL22
            Text1(iZielIndex).SetFocus
        Case Is = 5     'switch to lower case/upper case
            If Command4(5).Caption = "a>A" Then
                For lcount = 0 To 56
                    Command3(lcount).Caption = UCase$(Command3(lcount).Caption)
                Next lcount
                Command4(4).Caption = "A>a"
                Command4(5).Caption = "A>a"
            Else
                For lcount = 0 To 56
                    Command3(lcount).Caption = LCase$(Command3(lcount).Caption)
                Next lcount
                Command4(4).Caption = "a>A"
                Command4(5).Caption = "a>A"
            End If
            Text1(iZielIndex).SetFocus
    End Select
    
            
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Command4_Click"
    Fehler.gsFehlertext = "Im Programmteil Bedienerverwaltung ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub

Private Sub SwitchUpperLowerCaseWKL22()
    On Error GoTo LOKAL_ERROR
    
    Dim lcount As Long
    
    If Left(Command4(4).Caption, 1) = "A" Then
        For lcount = 22 To 32
            Command3(lcount).Caption = LCase(Command3(lcount).Caption)
        Next lcount
        For lcount = 35 To 45
            Command3(lcount).Caption = LCase(Command3(lcount).Caption)
        Next lcount
        For lcount = 47 To 55
            Command3(lcount).Caption = LCase(Command3(lcount).Caption)
        Next lcount
        Command3(54).Caption = ","
        Command3(55).Caption = "."
        Command3(56).Caption = "-"
        
        Command4(4).Caption = "a -> A"
    Else
        For lcount = 22 To 32
            Command3(lcount).Caption = UCase(Command3(lcount).Caption)
        Next lcount
        For lcount = 35 To 45
            Command3(lcount).Caption = UCase(Command3(lcount).Caption)
        Next lcount
        For lcount = 47 To 55
            Command3(lcount).Caption = UCase(Command3(lcount).Caption)
        Next lcount
        
        Command3(54).Caption = ";"
        Command3(55).Caption = ":"
        Command3(56).Caption = "_"
        
        Command4(4).Caption = "A -> a"
    
    End If
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "SwitchUpperLowerCaseWKL22"
    Fehler.gsFehlertext = "Im Programmteil Bedienerverwaltung ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
