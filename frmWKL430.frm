                cLiBesNr = ermgpEAN1(cArtNr)
                
                lBest = lbestmeng / lfakt
        
                cBest = Trim(lBest)
            End If
            
            cSatz = "line;" & cLiBesNr & ";" & cBest & ";"
            cSatz = cSatz & Chr$(13) & Chr$(10)
            lPos = LOF(iFileNr)
            lPos = lPos + 1
            Put #iFileNr, lPos, cSatz
        End If
    Next lrow
    
    Close iFileNr
    MsgBox "Datei " & cdatei & ".csv in " & cPfad & " erstellt!", vbInformation, "Winkiss Hinweis:"
    
Exit Sub
