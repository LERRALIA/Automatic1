            If Len(cSatz) >= 1024 Then
                cSchreiber = Left(cSatz, 1024) & vbLf
                
                lPos = LOF(iFileNr)
                lPos = lPos + 1
                Put #iFileNr, lPos, cSchreiber
                
                
                cSatz = Right(cSatz, Len(cSatz) - 1024)
            End If
