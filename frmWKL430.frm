    End If
    
    ArtikelmitkleinerenLEKandererLieferantenloeschen = False

    dMin = 0
    dMax = 0
    dtmp = 0
    '**************************** LEK ************************
     
    cSQL = "Select * from ARTLIEF where ARTNR = " & cArtNr
    cSQL = cSQL & " and linr = " & cLinr
    
