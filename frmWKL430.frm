            If dtmp < dMin Then
            
                sERG_Libesnr = ""
                sERG_MINMEN = "0"
                sLinr = ""
            
                dMin = dtmp
                cMin = Format$(dMin, "###,##0.00")
                If Not IsNull(rsrs!linr) Then
                    sLinr = rsrs!linr
                End If
                
                If Not IsNull(rsrs!LIBESNR) Then
                    sERG_Libesnr = rsrs!LIBESNR
                End If
                
                If Not IsNull(rsrs!MINMEN) Then
                    sERG_MINMEN = rsrs!MINMEN
                End If
            End If
            rsrs.MoveNext
        Loop
    End If
    rsrs.Close: Set rsrs = Nothing
    
