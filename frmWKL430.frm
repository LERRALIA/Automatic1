            MSFlexGrid2.Row = lrow
            MSFlexGrid2.TopRow = lrow
            MSFlexGrid2.SetFocus
            
            Screen.MousePointer = 0
        End If
        gcArtNrFiliale = ""
    End If
    
    If KeyCode = vbKeyF4 Then
        lrow = MSFlexGrid2.Row
