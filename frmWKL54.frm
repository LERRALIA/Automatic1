Private Sub ZeigeZugehˆrigeBediener(czuru As String, cPteil As String)
    On Error GoTo LOKAL_ERROR

    Dim cSQL        As String
    Dim rsrs        As Recordset
    Dim dberecht    As Double
    Dim dWert       As Double
    Dim cLBSatz     As String
    Dim cFeld       As String
    
    dberecht = CDbl(czuru)
    List2.Clear
    List2.AddItem "Nr. Bedienername"
    List1.Clear
    
    cSQL = "Select * from BEDNAME where BEDNU <> 99 and ( SYNSTATUS is null or SYNSTATUS = 'E' or SYNSTATUS = 'A' ) "
    cSQL = cSQL & " and bediener >= " & dberecht
    cSQL = cSQL & " order by BEDNU "
    Set rsrs = gdBase.OpenRecordset(cSQL)
    If Not rsrs.EOF Then
        rsrs.MoveFirst
        Do While Not rsrs.EOF
            If Not IsNull(rsrs!BEDNU) Then
                dWert = rsrs!BEDNU
            Else
                dWert = 0
            End If
            cFeld = Format$(dWert, "##0")
            cFeld = Space$(3 - Len(cFeld)) & cFeld
            cLBSatz = cFeld & " "
            
            If Not IsNull(rsrs!bedname) Then
                cFeld = rsrs!bedname
            Else
                cFeld = ""
            End If
            cFeld = Trim$(cFeld)
            cFeld = cFeld & Space$(35 - Len(cFeld)) & " "
            cLBSatz = cLBSatz & cFeld
            
            List1.AddItem cLBSatz
            
            rsrs.MoveNext
        Loop
    End If
    rsrs.Close: Set rsrs = Nothing
    Label1(0).Caption = cPteil
    Label1(0).Refresh
    
    Label3.Caption = List1.ListCount & " Bediener"
    Label3.Refresh
    
    Frame30.Top = Screen.Height / 2 - Frame30.Height / 2
    Frame30.Left = Screen.Width / 2 - Frame30.Width / 2
    Frame30.BackColor = glH2
    Frame30.Visible = True
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "ZeigeZugehˆrigeBediener"
    Fehler.gsFehlertext = "Im Programmteil Zugriffsrechte ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
