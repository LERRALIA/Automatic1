Private Sub SSCommand4_Click(Index As Integer)
    On Error GoTo LOKAL_ERROR
        
    Dim lCount          As Long
    Dim cLBSatz         As String
    Dim cFeld           As String
    Dim dSuchWert       As Double
    Dim dFindeWert      As Double
        
    Select Case Index
        Case 0 To 9    'Ziffern
            Text1(Label2(12).Caption).Text = Text1(Label2(12).Caption).Text & SSCommand4(Index).Caption
            Text1(Label2(12).Caption).SetFocus
            
        Case Is = 10    'Komma
            If InStr(Text1(Label2(12).Caption).Text, ",") = 0 Then
                Text1(Label2(12).Caption).Text = Text1(Label2(12).Caption).Text & SSCommand4(Index).Caption
            End If
            Text1(Label2(12).Caption).SetFocus
            
        Case Is = 11    'Clear
            Text1(Label2(12).Caption).Text = ""
            Text1(Label2(12).Caption).SetFocus
            
        Case Is = 12    'b
            Label2(12).Caption = 0
            Text1(0).SetFocus
        
        Case Is = 13    'l
            Label2(12).Caption = 1
            Text1(1).SetFocus
        
    End Select

Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "SSCommand4_Click"
    Fehler.gsFehlertext = "Es trat ein Fehler im Programmteil Maﬂe auf."
    
    Fehlermeldung1
    
End Sub

Private Sub Text1_Change(Index As Integer)
On Error GoTo LOKAL_ERROR
    
    Dim lbreite As Single
    Dim lLonge As Single
    Dim SiErgQM As Single
    Dim SiPreisQM As Single
    
    If IsNumeric(Text1(0).Text) Then
        lbreite = CSng(Text1(0).Text)
    End If

    If IsNumeric(Text1(1).Text) Then
        lLonge = CSng(Text1(1).Text)
    End If
    
    SiErgQM = lbreite * lLonge
    
    If IsNumeric(Label2(3).Caption) Then
        SiPreisQM = SiErgQM * CSng(Label2(3).Caption)
        Label2(9).Caption = Format$(SiErgQM, "######0.00")
