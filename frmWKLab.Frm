Attribute VB_Name = "frmWKLab"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Private Sub SchreibeVersionWKLab(cKasnum As String)
    On Error GoTo LOKAL_ERROR
    
    Dim iFileNr As Integer
    
    Kill gcPfad & "KASNUM.CFG"
    
    iFileNr = FreeFile
    Open gcPfad & "KASNUM.CFG" For Binary As iFileNr
    Put #iFileNr, 1, cKasnum
    gcKasNum = cKasnum
    Close iFileNr
    
    Do While Right(gcKasNum, 1) = vbCr Or Right(gcKasNum, 1) = vbLf
        gcKasNum = Left(gcKasNum, Len(gcKasNum) - 1)
    Loop
    
Exit Sub
LOKAL_ERROR:

    If err.Number = 53 Then
        Resume Next
    Else
        Fehler.gsDescr = err.Description
        Fehler.gsNumber = err.Number
        Fehler.gsFormular = Me.name
        Fehler.gsFunktion = "SchreibeVersionWKLab"
        Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
        
        Fehlermeldung1
    End If

End Sub
Private Sub Command1_Click(Index As Integer)
On Error GoTo LOKAL_ERROR

    Dim cVersion As String
    Dim iVersion As Integer

    Select Case Index
        Case 0 To 9
            Text1.Text = Text1.Text & Command1(Index).Caption
        Case 10
            Text1.Text = ""
            
        Case Is = 20
            cVersion = Text1.Text
            cVersion = Trim$(cVersion)
            If cVersion <> "" Then
                SchreibeVersionWKLab cVersion
                Unload frmWKLab
            End If
        Case Is = 21
            Unload frmWKLab
    End Select
    
    Screen.MousePointer = 0

    
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Command1_Click"
    Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
Private Sub Form_Load()
    On Error GoTo LOKAL_ERROR
    Dim i As Integer
    
    Screen.MousePointer = 11
    
    frmWKLab.Top = Screen.Height / 2 - frmWKLab.Height / 2
    frmWKLab.Left = Screen.Width / 2 - frmWKLab.Width / 2
    
    Text1.Text = ""
    
    For i = 0 To 8
        Command1(i).BackColorTo = glfarbe(i + 1)
        Command1(i).BackColorFrom = glfarbe(i + 1)
    Next i
    
    Screen.MousePointer = 0
    
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Form_Load"
    Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
    
    Fehlermeldung1
    
End Sub


Private Sub Text1_KeyPress(KeyAscii As Integer)
    On Error GoTo LOKAL_ERROR
    
    Dim cZeichen As String
    Dim cValid As String
    
    cValid = "123456789" & Chr$(8)
    
    cZeichen = Chr$(KeyAscii)
    If InStr(cValid, cZeichen) = 0 Then
        KeyAscii = 0
    End If
    
    
Exit Sub
