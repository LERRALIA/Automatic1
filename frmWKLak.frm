End Sub
Private Sub Command2_Click(Index As Integer)
On Error GoTo LOKAL_ERROR

    Screen.MousePointer = 11
    Dim iRet As Integer
    
    Select Case Index
        Case Is = 0     'Speichern
            iRet = fnPruefeGutscheinNr()
            Select Case iRet
                Case Is = 0     'alles okay
                
                    If Len(Text1.Text) = 8 Then
                        If gbGutschnrKomplett = True Then
                        
                        Else
                            Text1.Text = Mid(Text1.Text, 2, 6)
                        End If
                    ElseIf Len(Text1.Text) = 10 Then
                    
                    End If
                    glGutschNr = Val(Text1.Text)
                    Unload frmWKLak
                    
                Case Is = 1     'keine Eingabe
                    Screen.MousePointer = 11
                    MsgBox "Bitte geben Sie die Gutscheinnummer ein!", vbCritical, "STOP!"
                    Text1.SetFocus
                    
                Case Is = 2     'Nummer bereits vorhanden
                    Screen.MousePointer = 11
                    MsgBox "Diese Gutscheinnummer ist bereits vergeben!", vbCritical, "STOP!"
                    Text1.Text = ""
                    Text1.SetFocus
                Case Is = 99    'Nummer zu kurz, Eingriff vom Anwender
                    Screen.MousePointer = 11
                    Text1.SetFocus
            End Select
                
        Case Is = 1     'Schlieﬂen
            Unload frmWKLak
            
        Case Is = 2     'abbrechen
            gbabbruchG = True
            Unload frmWKLak
            
    End Select
    Screen.MousePointer = 0
Exit Sub
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "Command2_Click"
    Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Sub
