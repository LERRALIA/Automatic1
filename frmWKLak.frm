Private Function fnPruefeGutscheinNr() As Integer
    On Error GoTo LOKAL_ERROR
    
    Dim ctmp    As String
    Dim ctmp1   As String
    Dim lWert   As Long
    Dim cSQL    As String
    Dim rsrs    As Recordset
    Dim iRet    As Integer
    
    fnPruefeGutscheinNr = 0
    
    ctmp = Text1.Text
    ctmp = Trim$(ctmp)
    
    
    
    
    If Len(ctmp) = 8 Then
    
        If gbGutschnrKomplett = True Then
            'der bleibt so
            'gradmann neue 13er mit f¸hrender 2 werden zu 8er L‰nge
        Else
            ctmp = Mid(ctmp, 2, 6)
        End If
    ElseIf Len(Text1.Text) = 10 Then
    
    End If

    If ctmp = "" Then
        fnPruefeGutscheinNr = 1
        Exit Function
    End If
    
    lWert = Val(ctmp)
    
    
    
    If gbKL_LIVEGUTSCHEIN Then
    
    
    
    
    
    
        If fTestLogin_SQLDABA_Error = 0 Then 'ist alles OK? Datenbank erreichbar?
            'alles okay
            
        Else
        
            schreibeProtokollVPNTXT "Unterbrechung"
        
            Dim sTemp As String
            sTemp = "Bitte starten Sie diesen Rechner neu" & vbCrLf
            sTemp = sTemp & "oder schlieﬂen Sie das Schloss und starten Sie WinKiss neu."
        
            MsgBox sTemp, vbCritical + vbOKOnly, "Gutschein-Datenbank nicht erreichbar"
            fnPruefeGutscheinNr = 1
            Exit Function
        End If
    
    
    
    
    
    
        Dim stConnect As String
        
        
        
        If gsKL_DSN <> "" Then
            stConnect = "ODBC;DSN=" & gsKL_DSN & ";UID=" & gsKL_BENUTZER & ";PWD=" & gsKL_PASSWORT & ""
        Else
            stConnect = "ODBC;DRIVER=SQL Server;SERVER=" & gsKL_ADRESSE & ";DATABASE=" & gsKL_DATENBANKNAME & ";UID=" & gsKL_BENUTZER & ";PWD=" & gsKL_PASSWORT & ""
        End If
        
        
        
        
        
        Dim dbEAN As DAO.Database
        Set dbEAN = OpenDatabase(gsKL_DATENBANKNAME, dbDriverNoPrompt, False, stConnect)
        
        
        
        cSQL = "Select * from GUTSCHEINE where GUTSCHNR = '" & CStr(lWert) & "'"
        Set rsrs = dbEAN.OpenRecordset(cSQL)
        If Not rsrs.EOF Then
            fnPruefeGutscheinNr = 2
            Exit Function
        End If
        rsrs.Close: Set rsrs = Nothing
        
        dbEAN.Close
    Else

        cSQL = "Select * from GUTSCH where GUTSCHNR = " & lWert
        Set rsrs = gdBase.OpenRecordset(cSQL)
        If Not rsrs.EOF Then
            fnPruefeGutscheinNr = 2
            Exit Function
        End If
        rsrs.Close: Set rsrs = Nothing
        
    End If
    
Exit Function
LOKAL_ERROR:
    Fehler.gsDescr = err.Description
    Fehler.gsNumber = err.Number
    Fehler.gsFormular = Me.name
    Fehler.gsFunktion = "fnPruefeGutscheinNr"
    Fehler.gsFehlertext = "Es ist ein Fehler aufgetreten."
    
    Fehlermeldung1
End Function
